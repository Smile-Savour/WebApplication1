<Window x:Class="WPFExample_WpfAndJs.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFExample_WpfAndJs"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="800" ContentRendered="Window_ContentRendered">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="500"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
        </Grid.RowDefinitions>
        <WebBrowser Grid.Row="0" Name="wbrExam" Height="500" VerticalAlignment="Top" Width="800"/>
        <Button Grid.Row="1" Content="CS调用JS" VerticalAlignment="Top" Height="23" Margin="10,10,719,0" Click="Button_Click" />
        <Button Grid.Row="1" Content="red" VerticalAlignment="Top" Height="23" Margin="78,10,650,0" Click="ChangeColor" />
        <Button Grid.Row="1" Content="yellow" VerticalAlignment="Top" Height="23" Margin="216,10,512,0" Click="ChangeColor" />
        <Button Grid.Row="1" Content="green" VerticalAlignment="Top" Height="23" Margin="147,10,581,0" Click="ChangeColor" />
        <Button Grid.Row="1" Content="gray" VerticalAlignment="Top" Height="23" Margin="285,10,443,0" Click="ChangeColor" />
        <Button Grid.Row="1" Content="blue" VerticalAlignment="Top" Height="23" Margin="354,10,374,0" Click="ChangeColor" />
        <Button Grid.Row="1" Content="white" VerticalAlignment="Top" Height="23" Margin="423,10,305,0" Click="ChangeColor" />
        <Button Grid.Row="1" Content="black" VerticalAlignment="Top" Height="23" Margin="492,10,236,0" Click="ChangeColor" />
        <Button Grid.Row="1" Content="purple" VerticalAlignment="Top" Height="23" Margin="561,10,167,0" Click="ChangeColor" />
        <Button Grid.Row="1" Content="brown" VerticalAlignment="Top" Height="23" Margin="630,10,98,0" Click="ChangeColor" />
        <Button Grid.Row="1" Content="pink" VerticalAlignment="Top" Height="23" Margin="699,10,29,0" Click="ChangeColor" />

    </Grid>
</Window>
